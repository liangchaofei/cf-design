AU,CAAC,KAAD,CAAV;AACAH,IAAAA,OAAO,IAAIA,OAAO,EAAlB;AACH,GAHD;;AAZwC,mBAgBXV,QAAQ,CAAS,SAAT,CAhBG;AAAA;AAAA,MAgBjCe,SAhBiC;AAAA,MAgBtBC,OAhBsB;;AAiBxCf,EAAAA,SAAS,CAAC,YAAM;AACZ,YAAQI,IAAR;AACI,WAAK,SAAL;AACIW,QAAAA,OAAO,CAAC,qBAAD,CAAP;AACA;;AACJ,WAAK,OAAL;AACIA,QAAAA,OAAO,CAAC,mBAAD,CAAP;AACA;;AACJ,WAAK,MAAL;AACIA,QAAAA,OAAO,CAAC,kBAAD,CAAP;AACA;;AACJ,WAAK,MAAL;AACIA,QAAAA,OAAO,CAAC,kBAAD,CAAP;AACA;;AACJ;AACIA,QAAAA,OAAO,CAAC,qBAAD,CAAP;AACA;AAfR;AAiBH,GAlBQ,EAkBN,CAACD,SAAD,CAlBM,CAAT;AAmBA,SACI;AAAK,IAAA,SAAS,uBAAgBA,SAAhB,CAAd;AAA2C,IAAA,KAAK;AAAIE,MAAAA,OAAO,EAAEL,OAAO,GAAG,OAAH,GAAa;AAAjC,OAA4CD,KAA5C,CAAhD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAK,IAAA,SAAS,EAAC,YAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAA6BJ,OAA7B,CADJ,EAEI;AAAK,IAAA,SAAS,EAAC,aAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACIC,WADJ,CAFJ,EAMQF,QAAQ,IACR;AAAM,IAAA,SAAS,EAAC,cAAhB;AAA+B,IAAA,OAAO,EAAEQ,OAAxC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAkDL,SAAS,GAAGA,SAAH,GAAe,GAA1E,CAPR,CADJ;AAaH,CAjDa,mGAAd;eAmDeP,K;AAAf;;;;;;;;;;;;;;;;;;;;;;0BAhEQF,Q;0BAAUC,S;0BAaZC,K","sourcesContent":["import React from 'react';\nimport './index.less'\nconst { useState, useEffect } = React;\n\ninterface IProps {\n    style: Object; // 自定义样式\n    closable: boolean; // 是否显示关闭按钮，默认不显示\n    closeText: string | any; // 自定义关闭按钮\n    message: string; // 警告提示内容\n    description: string; // 警告提示的辅助性文字\n    type: string; // 警告的类型\n    onClose: () => void; // 关闭时触发的事件\n}\n\n\nconst Alert = React.memo((props: IProps) => {\n    const {\n        type = 'warn',\n        closable = false,\n        message,\n        description,\n        closeText,\n        onClose,\n        style\n    } = props;\n    const [visible, setVisible] = useState<boolean>(true)\n\n    const closeCb = () => {\n        setVisible(false)\n        onClose && onClose()\n    }\n    const [typeStyle, setType] = useState<string>('success')\n    useEffect(() => {\n        switch (type) {\n            case 'success':\n                setType('cfAlertSuccessStyle')\n                break;\n            case 'error':\n                setType('cfAlertErrorStyle')\n                break;\n            case 'info':\n                setType('cfAlertInfoStyle')\n                break;\n            case \"warn\":\n                setType('cfAlertWarnStyle')\n                break;\n            default:\n                setType('cfAlertSuccessStyle')\n                break;\n        }\n    }, [typeStyle])\n    return (\n        <div className={`cfAlertBox ${typeStyle}`} style={{ display: visible ? \"block\" : \"none\", ...style }}>\n            <div className=\"cfAlertMsg\">{message}</div>\n            <div className=\"cfAlertHelp\">{\n                description\n            }</div>\n            {\n                closable &&\n                <span className=\"cfAlertClose\" onClick={closeCb}>{closeText ? closeText : 'x'}</span>\n            }\n\n        </div>\n    )\n})\n\nexport default Alert;"]}]}