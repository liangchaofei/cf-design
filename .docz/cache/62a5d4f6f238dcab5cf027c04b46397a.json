CAAT;AAQAxB,EAAAA,SAAS,CAAC,YAAM;AACd,QAAG,CAACwB,QAAJ,EAAc;AACZc,MAAAA,QAAQ,CAACG,IAAT,CAAcC,KAAd,CAAoBC,QAApB,GAA+B,QAA/B;AACD;AACF,GAJQ,EAIN,CAACnB,QAAD,CAJM,CAAT;;AAMA,MAAMK,WAAW,GAAG,SAAdA,WAAc,CAACe,EAAD,EAAQ;AAC1BnB,IAAAA,SAAS,CAAC,YAAM;AACdmB,MAAAA,EAAE,IAAIA,EAAE,EAAR;AACA,aAAO,IAAP;AACD,KAHQ,CAAT;;AAIA,QAAGjC,cAAH,EAAkB;AAChBgB,MAAAA,eAAe,CAAC,IAAD,CAAf;AACD;;AACDW,IAAAA,QAAQ,CAACG,IAAT,CAAcC,KAAd,CAAoBC,QAApB,GAA+B,MAA/B;AACD,GATD,CA9DyC,CAwEzC;;;AACA,MAAME,QAAQ,GAAG,SAAXA,QAAW,GAAM;AACrBhB,IAAAA,WAAW,CAAC,IAAD,CAAX;AACD,GAFD;;AAGA,SACE;AAAK,IAAA,SAAS,EAAC,YAAf;AAA4B,IAAA,KAAK,EAAE;AAACiB,MAAAA,OAAO,EAAEtB,QAAQ,GAAG,MAAH,GAAU;AAA5B,KAAnC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,yBAAkBhB,QAAQ,GAAE,YAAF,GAAgB,EAA1C,CAAd;AAA8D,IAAA,KAAK,EAAE;AAACY,MAAAA,KAAK,EAALA;AAAD,KAArE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAC,cAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAC,aAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACGF,KADH,CADF,CADF,EAOIT,QAAQ,IACR;AAAM,IAAA,SAAS,EAAC,gBAAhB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACGC,SADH,CARJ,EAYE;AAAK,IAAA,SAAS,EAAC,YAAf;AAA4B,IAAA,KAAK,EAAEJ,SAAnC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACGoB,YAAY,GAAG,IAAH,GAASH,QADxB,CAZF,EAgBIX,MAAM,KAAK,IAAX,GAAkB,IAAlB,GACA;AAAK,IAAA,SAAS,EAAC,cAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAEIA,MAAM,GAAGA,MAAH,GACN;AAAK,IAAA,SAAS,EAAC,YAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAC,kBAAf;AAAkC,IAAA,OAAO,EAAEgB,WAA3C;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAyDrB,UAAzD,CADF,EAEE;AAAK,IAAA,SAAS,EAAC,cAAf;AAA8B,IAAA,OAAO,EAAEsC,QAAvC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAkD5B,MAAlD,CAFF,CAHJ,CAjBJ,CADF,EA+BIH,IAAI,IAAI;AAAK,IAAA,SAAS,EAAC,YAAf;AAA4B,IAAA,KAAK,EAAEE,SAAnC;AAA8C,IAAA,OAAO,EAAED,YAAY,IAAIa,WAAvE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IA/BZ,CADF;AAoCD,CAhHa,mJAAd;eAkHe1B,K;AAAf;;;;;;;;;;;;;;;;;;;;;;0BA3IQH,Q;0BAAUC,S;0BAwBdC,W;0BACEC,K","sourcesContent":["import React from 'react';\nimport './index.less'\nconst { useState, useEffect} = React;\n\ninterface IProps {\n  afterClose:any; //Modal完全关闭后的回调\n  bodyStyle:object; //Modal body的样式\n  cancelText: string | any; // 取消按钮文字\n  centered: boolean; // 是否居中展示Modal\n  closable: boolean; // 是否展示右上角的关闭按钮\n  closeIcon:any; // 自定义关闭图标\n  destroyOnClose: boolean; // 是否关闭时销毁Modal里的子元素\n  footer:null | any; //底部内容，当不需要底部默认按钮时，可以设置为footer={null}\n  keyboard: boolean; //是否支持键盘的esc键退出\n  mask: boolean; // 是否展示遮罩\n  maskclosable: boolean; // 点击蒙层是否允许关闭\n  maskStyle: any; //遮罩样式\n  okText: string | any; // 确认按钮的文本\n  title: string | any; // 标题\n  visible: boolean; // Modal是否可见\n  width: string; // Modal宽度\n  onCancel: any; //点击遮罩或者取消按钮，或者键盘esc按键时的回调\n  onOk: any; // 点击确定的回调\n  children: any;\n}\n\nlet hiddenCount = 0;\nconst Modal = React.memo((props:IProps) => {\n  const {\n    afterClose,\n    bodyStyle,\n    cancelText = '取消',\n    centered,\n    closable = true,\n    closeIcon,\n    destroyOnClose,\n    footer,\n    keyboard,\n    mask = true,\n    maskclosable = true,\n    maskStyle,\n    okText = '确认',\n    title,\n    visible = false,\n    width = '520px',\n    onCancel,\n    onOk,\n    children\n  } = props;\n  let [isHidden,setHidden] = useState<boolean>(!visible)\n  let [destroyChild,setDestoryChild] = useState<boolean>(false)\n  const handleClose = () => {\n    hiddenModal(onCancel)\n  }\n\n  useEffect(() => {\n    if(visible && destroyOnClose){\n      setDestoryChild(true)\n    }\n  },[visible,destroyOnClose])\n\n\n  const closeModal = function (event) {\n    let e = event || window.event || arguments.callee.caller.arguments[0]\n    if (e && e.keyCode === 27) { \n      handleClose()\n    }\n  }\n  useEffect(() => {\n    document.addEventListener('keydown',closeModal,false)\n    return () => {\n      document.removeEventListener('keydown',closeModal,false)\n    }\n  },[])\n\n  useEffect(() => {\n    if(isHidden && hiddenCount){\n      hiddenCount = 0;\n      afterClose && afterClose()\n    }\n    hiddenCount = 1;\n  },[isHidden])\n\n  useEffect(() => {\n    if(!isHidden) {\n      document.body.style.overflow = 'hidden'\n    }\n  }, [isHidden])\n  \n  const hiddenModal = (cb) => {\n    setHidden(() => {\n      cb && cb()\n      return true;\n    })\n    if(destroyOnClose){\n      setDestoryChild(true)\n    }\n    document.body.style.overflow = 'auto';\n  }\n  // 成功回调\n  const handleOk = () => {\n    hiddenModal(true)\n  }\n  return (\n    <div className=\"xModalWrap\" style={{display: isHidden ? 'none':'block'}}>\n      <div className={`xModalContent${centered ?' xCentered': ''}`} style={{width}}>\n        <div className=\"xModalHeader\">\n          <div className=\"xModalTitle\">\n            {title}\n          </div>\n        </div>\n        {\n          closable && \n          <span className=\"xModalCloseBtn\">\n            {closeIcon }\n          </span>\n        }\n        <div className=\"xModalBody\" style={bodyStyle}>\n          {destroyChild ? null :children}\n        </div>\n        {\n          footer === null ? null :\n          <div className=\"xModalFooter\">\n            {\n              footer ? footer:\n              <div className=\"xFooterBtn\">\n                <div className=\"xFooterBtnCancel\" onClick={handleClose}>{cancelText}</div>\n                <div className=\"xFooterBtnOk\" onClick={handleOk}>{okText}</div>\n              </div>\n            }\n          </div>\n        }\n       \n      </div>\n      {\n        mask && <div className=\"xModalMask\" style={maskStyle} onClick={maskclosable && handleClose}></div>\n      }\n  </div>\n  )\n})\n\nexport default Modal;"]}]}