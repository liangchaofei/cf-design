AAOF,CAAAA,GAAG,CAAGD,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAAN,CAAoBA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAApB,CACHA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,EAAcC,GAAd,EAAqBA,GAAG,CAAGD,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAA3B,CAAyCA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAAzC,CACEE,YAFN,CAGD,CAJK,IAIA,IAAGF,KAAK,CAACG,MAAN,GAAiB,CAApB,CAAuB,CAC3B,MAAOF,CAAAA,GAAG,CAAGD,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAAN,CAAoBA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAApB,CACHA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,EAAcC,GAAd,EAAqBA,GAAG,CAAGD,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAA3B,CAAyCA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAAzC,CACEA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,EAAcC,GAAd,EAAqBA,GAAG,CAAGD,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAA3B,CAAyCA,KAAK,CAAC,CAAD,CAAL,CAAS,CAAT,CAAzC,CACEE,YAHR,CAID,CACF,CAED,GAAME,CAAAA,QAAQ,CAAGX,KAAK,CAACY,IAAN,CAAW,SAACC,KAAD,CAAmB,uBAStCA,KATsC,CAEvCC,UAFuC,CAEvCA,UAFuC,4BAE5B,MAF4B,mBAGvCC,OAHuC,CAStCF,KATsC,CAGvCE,OAHuC,CAIvCC,UAJuC,CAStCH,KATsC,CAIvCG,UAJuC,mBAStCH,KATsC,CAKvCI,UALuC,CAKvCA,UALuC,4BAK1B,KAL0B,gCAStCJ,KATsC,CAMvCK,KANuC,CAMvCA,KANuC,uBAM/B,OAN+B,cAOvCC,SAPuC,CAStCN,KATsC,CAOvCM,SAPuC,CAQvCC,WARuC,CAStCP,KATsC,CAQvCO,WARuC,CAU3C,MACIJ,CAAAA,UAAU,EAAID,OAAO,GAAI,GAAzB,CAA8B,IAA9B,CACA,2BAAK,SAAS,CAAC,cAAf,EACI,2BAAK,SAAS,CAAC,cAAf,CAA8B,KAAK,CAAE,CAACG,KAAK,CAAC,MAAOA,CAAAA,KAAP,GAAiB,QAAjB,CAA4BA,KAAK,CAAG,IAApC,CAA2CA,KAAlD,CAArC,EACI,2BAAK,SAAS,CAAC,iBAAf,CAAiC,KAAK,CAAE,CAACA,KAAK,WAAIH,OAAJ,KAAN,CAAqBM,eAAe,CAAED,WAAW,EAAIA,WAAW,CAACV,MAA3B,CAAoCJ,WAAW,CAACc,WAAD,CAAcL,OAAd,CAAuBD,UAAvB,CAA/C,CAAoFA,UAA1H,CAAxC,EADJ,CADJ,CAKQ,CAACG,UAAD,EACA,4BAAM,SAAS,CAAC,cAAhB,CAA+B,KAAK,CAAE,CAACK,KAAK,CAACH,SAAP,CAAtC,EAA0DJ,OAA1D,KANR,CAFJ,CAaH,CAvBgB,CAAjB,CAyBA,cAAeJ,CAAAA,QAAf,C","sourcesContent":["import React from 'react';\nimport './index.less';\ninterface IProps{\n    themeColor: string; //进度条颜色\n    percent: number; // 进度值百分比\n    autoHidden: boolean; //是否进度到100%时自动消失\n    hiddenText: boolean; // 是否隐藏进度条文本\n    width: string | number; // 进度条宽度\n    textColor: string;  // 进度文本颜色\n    statusScope: number[]; //状态阈值,分别设置不同进度范围的进度条颜色,最大允许设置3个值, 为一个二维数组\n}\n\n// 升序排序\nlet sortArr = (arr:any[]) => arr.sort((a,b) => a[0] - b[0])\n\n// 检测值所对应的进度条颜色状态\nfunction checkStatus(scope:any[], val:number, defaultColor:string) {\n  val = +val\n  // 从小到大排序\n  sortArr(scope)\n\n  if(scope.length === 1) {\n    return val < scope[0][0] ? scope[0][1] : defaultColor\n  }else if(scope.length === 2) {\n    return val < scope[0][0] ? scope[0][1]\n      : scope[0][0] < val && val < scope[1][0] ? scope[1][1]\n        : defaultColor\n  }else if(scope.length === 3) {\n    return val < scope[0][0] ? scope[0][1]\n      : scope[0][0] < val && val < scope[1][0] ? scope[1][1]\n        : scope[1][0] < val && val < scope[2][0] ? scope[2][1]\n          : defaultColor\n  }\n}\n\nconst Progress = React.memo((props: IProps) => {\n    const {\n        themeColor='blue',\n        percent,\n        autoHidden,\n        hiddenText = false,\n        width = '100px',\n        textColor,\n        statusScope\n     } = props;\n    return (\n        autoHidden && percent ===100 ?null:\n        <div className=\"cfProcessBox\">\n            <div className=\"cfProcessBar\" style={{width:typeof width === 'number' ? width + 'px' : width}}>\n                <div className=\"cfProcessBarTwo\" style={{width:`${percent}%`,backgroundColor: statusScope && statusScope.length ? checkStatus(statusScope, percent, themeColor) : themeColor}}></div>\n            </div>\n            {\n                !hiddenText && \n                <span className=\"cfProcessNum\" style={{color:textColor}}>{percent}%</span>\n            }\n    \n        </div>\n    )\n})\n\nexport default Progress;"]}]}